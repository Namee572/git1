package ex03;

public class Ex04 {
	
	public static void main(String[] args) {
		System.out.print("ln이 없으면 줄바꿈이 안됩니다.");
		System.out.println("======");
		System.out.println("ln이 있으면 줄바꿈이 됩니다.");
		System.out.print("======");
		
		System.out.println("**********");
		System.out.print("공부 한다고\n 고생이셈\n");
		System.out.print("잘될거셈....\n");
		System.out.println("줄바꿈\n역슬래쉬플러스앤n\n임\n");
		
		
		
	}

}
