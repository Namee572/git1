package ex02;

import java.util.*;

// 7장 맛보기
class AA{
	public void doA() {
		System.out.println("AA.doA()");
	}
}

public class Ex07 {

	public static void main(String[] args) {
		// 기본형
		int a = 10;
		// 참조형 타입 변수 선언
		Scanner scanner = new Scanner(System.in);
		
		AA aa = new AA();
		aa.doA();
//		scan.nextInt();
		
		
	}
}
