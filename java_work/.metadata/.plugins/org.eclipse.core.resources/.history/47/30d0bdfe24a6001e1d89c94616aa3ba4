package aa;

class VarEx1 {
	public static void main(String[] args) {
		int year = 0;
		int age  = 14;
		 
		System.out.println(year);
		System.out.println(age);

		year = age + 2000;  // ���� age�� ���� 2000�� ���ؼ� ���� year�� ����
		age  = age + 1;     // ���� age�� ����� ���� 1������Ų��.

		System.out.println(year);
		System.out.println(age);
	}
}
