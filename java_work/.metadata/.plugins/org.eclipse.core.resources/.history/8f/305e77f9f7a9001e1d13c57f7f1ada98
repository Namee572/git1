package baek;

public class Main{
	
    public static void main(String[] args){
    	
        int A = 7;
        int B = 3;
    
        System.out.println(A+B);
        System.out.println(A-B);
        System.out.println(A*B);
        System.out.println(A/B);
        System.out.println(A%B);
      
	}
}
