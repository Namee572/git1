package ex03;

public class Ex07 {
	
/*
 * int -> char unicode A => 65 65 => A
 * 
 * 실수 -> 정수
 * double -> int 소수점을 버려서 형변환됩니다.	
 */
	public static void main(String[] args) {
		double d = 85.4d;
		int score = (int)d; //실수 d를 정수형으로 출력해조
		System.out.println("score = "+score);
		System.out.println("d = "+d);
	}

}
