package ex01;

// 재생버튼
// 마우스 우클릭 Run as -> Java application
// ctrl + F11
public class Ex02 {

	public static void main(String[] args) {
		
		int a = 10;
		System.out.println("a = " + a);
		
		int b = 20;
		System.out.println("a + b = "+a+b);
	
		String c = "a";
		String d = "b";
		System.out.println(c+d);
		
	}
}
