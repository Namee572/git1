package ex03_1;

public class Ex01 {
	
	public static void main(String[] args) {
		System.out.println(3%2);
		
		// 0010 2진수
		// 1000 => 8 (<< 비트연산)
		System.out.println(2<<2);
		
		System.out.println(10>3);
		System.out.println(10<=3);
		
		System.out.println(10==3);
		System.out.println(10!=3);
	}

}
